H5FileName         = fields [ Output file name ]
NumberOfDimensions = 3        [ Number of dimensions ]
FirstCycle         = 0        [ First cycle ]
LastCycle          = 10        [ Last cycle ]
StepBetweenCycles  = 2        [ Cycle interval ]

OriginInX          = 0.00     [ Origin in X ]
OriginInY          = 0.00     [ Origin in Y ]
OriginInZ          = 0.00     [ Origin in Z ]
LenghtInX          = 2.56     [ Domain size in X ]
LenghtInY          = 2.56     [ Domain size in Y ]
LenghtInZ          = 0.04     [ Domain size in Z ]

NumberOfCellsInX   = 128      [ Number of cells in X ]
NumberOfCellsInY   = 128      [ Number of cells in Y ]
NumberOfCellsInZ   = 2        [ Number of cells in Z ]

NumberOfFields     = 5        [ Number of fields to write ]
Field_1            = rho_0 /Step#0/Block/rho0/0
Field_2            = rho_1 /Step#0/Block/rho1/0
Field_3            = Ex    /Step#0/Block/Exn/0
Field_4            = Ey    /Step#0/Block/Eyn/0
Field_5            = Ez    /Step#0/Block/Ezn/0
